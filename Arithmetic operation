// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int a,b;
    //add and subract and multiply
    printf("Enter the two numbewr ");
    scanf("%d %d",&a,&b);
    printf("addition of two number =%d\n",a+b);
    printf("subration of two number =%d\n",a-b);
    printf("multiplication of two number=%d",a*b);
    printf("division of two number=%d",a/b);
    return 0;
}
