// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int a,b;
    //add and subract and multiply
    printf("Enter the number to square and cube ");
    scanf("%d",&b);
    printf("square of  number =%d\n",b*b);
    printf("cube of  number =%d\n",b*b*b);
    
    return 0;
}
